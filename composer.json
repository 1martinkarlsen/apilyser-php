{
    "name": "1martinkarlsen/apilyser",
    "description": "API documentation and code validator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Karlsen",
            "email": "1martinkarlsen@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Apilyser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apilyser\\tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/console": "^7.2",
        "nikic/php-parser": "^5.3",
        "symfony/yaml": "^7.2",
        "symfony/config": "^7.3",
        "symfony/http-foundation": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.5"
    },
    "bin": [
        "bin/apilyser"
    ],
    "scripts": {
        "test": "phpunit tests"
    }
}
