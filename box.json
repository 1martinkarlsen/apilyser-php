{
    "main": "bin/apilyser",
    "output": "apilyser.phar",
    "directories": ["src"],
    "finder": [
        {
            "name": "*.php",
            "in": "vendor",
            "exclude": ["Tests", "tests", "test"]
        }
    ],
    "finder-bin": [
        {
            "name": "*",
            "in": "vendor/symfony",
            "exclude": ["*.php"]
        }
    ],
    "compression": "GZ"
}