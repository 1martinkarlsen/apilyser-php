{
    "main": "bin/apilyser",
    "output": "apilyser.phar",
    "directories": ["src", "vendor"],
    "finder": [
        {
            "name": ["*.php", "*.json", "*.xml"],
            "in": "vendor"
        }
    ],
    "finder-bin": [
        {
            "name": "*",
            "exclude": ["*.php"],
            "in": "vendor/symfony"
        }
    ],
    "compression": "GZ"
}